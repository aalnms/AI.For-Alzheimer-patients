{
  "expo": {
    "name": "AI.For-Alzheimer-patients",
    "slug": "AI.For-Alzheimer-patients",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/602956D9-F62A-4D03-A601-4EC17D11E09B.png",
    "scheme": "aiforalzheimerpatients",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    }, 
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "router": {
      "extraRoutes": [
        "/scr/session-wizard"
      ]
    }
  }
}
